Host skippy-*
    User ebon
    IdentityFile ~/.ssh/id_ed25519
    ServerAliveInterval 60
    ServerAliveCountMax 3
    Compression yes
    ControlMaster auto
    ControlPath ~/.ssh/control:%h:%p:%r
    ControlPersist 1h

Host skippy-local
    HostName 10.0.0.21

Host skippy-domain
    HostName eboneth.xyz 

Host skippy-ddns
    HostName eboneth.ddns.net

Host skippy-vpn
    HostName 10.0.0.21
    ProxyCommand ssh your-vpn-server -W %h:%p
